                <li>Use JavaScript to fetch the data:</li>
            </ol>
            
            <div class="code-container">
                <pre><code>// Example JavaScript code to fetch and display movies
fetch('movies.json')
  .then(response => response.json())
  .then(data => {
    // Process and display the movies
    displayMovies(data.now_showing, 'now-showing');
    displayMovies(data.new_this_week, 'new-this-week');
    displayMovies(data.coming_soon, 'coming-soon');
  })
  .catch(error => console.error('Error loading movie data:', error));

function displayMovies(movies, sectionId) {
  const section = document.getElementById(sectionId);
  // Clear existing content
  section.innerHTML = '';
  
  // Add each movie to the section
  movies.forEach(movie => {
    const movieElement = createMovieElement(movie);
    section.appendChild(movieElement);
  });
}

